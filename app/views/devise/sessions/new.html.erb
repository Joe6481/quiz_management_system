<strong>
  <p id="sign_in_message">Welcome to the Quiz Management Site, from WebbiSkools Ltd. </p>
  <h2 id="log_in_below">Please log in below</h2>
<strong>


<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="log_in_form_inputs">
    <h3>
      <%= f.input :username, required: true, autofocus: true, input_html: { autocomplete: "username" } %>
      <br>
      <%= f.input :password, required: true, input_html: { autocomplete: "current-password" } %>
    </h3>
  </div>
  
  <div class="remember_me">
    <%= f.label "Remember Me?" do %>
      Remember Me?
      <%= f.check_box :remember_me, as: :boolean if devise_mapping.rememberable? %>
    <% end %>
  </div>

  <br>
  <br>

  <div class="form-actions">
    <%= f.button :submit, "Log in", class: "submit" %>
  </div>
<% end %>
